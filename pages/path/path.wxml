<!--pages/path/path.wxml-->
<block class=".kind-list-item-hd" wx:for-items="{{list}}" wx:key="{{item.id}}">
  <view id="{{item.id}}" class="kind-list-item" bindtap="kindToggle">
    <view class="kind-list-text">{{item.name}}</view>
    <image class="kind-list-img" src="resources/feedback.png"></image>
  </view>
</block>
